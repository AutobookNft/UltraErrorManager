{
    "name": "ultra/errormanager",
    "description": "Sistema avanzato di gestione errori di livello Enterprise per applicazioni Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Fabio Cherici",
            "email": "fabiocherici@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "illuminate/support": "^11.0",
        "illuminate/log": "^11.0",
        "illuminate/http": "^11.0",
        "illuminate/mail": "^11.0",
        "illuminate/view": "^11.0",
        "ultra/ultra-log-manager": "dev-main",
        "ultra/ultra-config-manager": "dev-main"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5.3",
        "mockery/mockery": "^1.6",
        "orchestra/testbench": "^9.0",
        "fakerphp/faker": "^1.23"
    },
    "autoload": {
        "psr-4": {
            "Ultra\\ErrorManager\\": "src/"
        },
        "files": [
            "src/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Ultra\\ErrorManager\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ultra\\ErrorManager\\Providers\\UltraErrorManagerServiceProvider"
            ],
            "aliases": {
                "UltraError": "Ultra\\ErrorManager\\Facades\\UltraError",
                "TestingConditions": "Ultra\\ErrorManager\\Facades\\TestingConditions"
            }
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/AutobookNft/UltraLogManager"
        },
        {
            "type": "vcs",
            "url": "https://github.com/AutobookNft/UltraConfigManager"
        }
        
    ],
    "minimum-stability": "dev", 
    "prefer-stable": true 

}
